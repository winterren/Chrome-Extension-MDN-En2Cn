{
  "manifest_version": 2,

  "name": "MDN English2Chinese",
  "description": "1.地址栏输入mdn按Tab简易搜索，2.选中文字右键简易搜索，3.点击图标英翻中",
  "version": "1.2",
  "omnibox": { "keyword" : "mdn" },
  "background": {
    "scripts": ["mdn.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs"
  ]
}